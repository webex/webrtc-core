{
  "source": {
    "include": ["src"],
    "includePattern": ".ts$",
    "excludePattern": "(node_modules/|docs|mocks)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs-jsdoc/",
    "readme": "./readme.md"
  }
}
